  },
  "keywords": [
    "travel"
  ],
  "author": "Luke Blaney",
  "license": "N/A",
  "bugs": {
    "url": "https://github.com/lucas42/lucos_transport/issues"
  },
  "homepage": "https://github.com/lucas42/lucos_transport",
  "devDependencies": {
    "ava": "^6.1.1",
    "browserify": "^17.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "fs-readfile-promise": "^3.0.1",
    "htmlparser2": "^9.1.0",
    "lucos_pubsub": "^0.0.4",
    "mespeak": "^2.0.2",
    "moment-timezone": "^0.5.44",
    "mustache": "^4.2.0",
    "mustache-express": "^1.3.2",
    "node-fetch": "^2.7.0",
    "tap-xunit": "^2.4.1",
